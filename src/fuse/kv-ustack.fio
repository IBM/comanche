; -- start job file --
[global]
ioengine=psync
iodepth=1
bs=${BS}
direct=1; O_DIRECT
sync=${SYNC}; O_SYNC
size=${SIZE}
numjobs=1
directory=${DIRECTORY}
thread ;unless it will fork
iomem=mmaphuge
hugepage-size=2m
time_based
runtime=10

; [random-write]
; rw=randwrite

[sequential-write]
rw=write

; [verify]
; verify=md5
; verify_pattern=%o

;[random-read]
; rw=randread

; -- end job file --
