; -- start job file --
[global]
ioengine=psync
iodepth=1
bs=32k
direct=1; O_DIRECT
sync=1; O_SYNC
size=2m
numjobs=1
directory=/tmp/kvfs
;directory=/tmp/kvfs-ustack ; kvustack
thread ;unless it will fork
iomem=mmaphuge
hugepage-size=2m

[random-write]
rw=randwrite

;[random-read]
; rw=randread

; -- end job file --
